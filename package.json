{
    "name": "nuxt-module-tauri",
    "version": "0.1.2",
    "description": "Nuxt module for Tauri applications",
    "repository": "onivoid/nuxt-module-tauri",
    "author": "Onivoid",
    "homepage": "https://github.com/onivoid/nuxt-module-tauri",
    "bugs": "https://github.com/onivoid/nuxt-module-tauri/issues",
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/types.d.mts",
            "import": "./dist/module.mjs"
        }
    },
    "main": "./dist/module.mjs",
    "typesVersions": {
        "*": {
            ".": [
                "./dist/types.d.mts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "prepack": "nuxt-module-build build",
        "dev": "npm run dev:prepare && nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
        "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
        "lint": "eslint .",
        "test": "vitest run",
        "test:watch": "vitest watch",
        "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
    },
    "dependencies": {
        "@nuxt/kit": "^4.0.1"
    },
    "devDependencies": {
        "@nuxt/devtools": "^2.6.2",
        "@nuxt/eslint-config": "^1.7.1",
        "@nuxt/module-builder": "^1.0.2",
        "@nuxt/schema": "^4.0.1",
        "@nuxt/test-utils": "^3.19.2",
        "@tauri-apps/api": "^2.7.0",
        "@types/node": "latest",
        "changelogen": "^0.6.2",
        "eslint": "^9.32.0",
        "nuxt": "^4.0.1",
        "typescript": "~5.8.3",
        "vitest": "^3.2.4",
        "vue-tsc": "^3.0.4"
    },
    "peerDependencies": {
        "@tauri-apps/api": "^2.7.0"
    },
    "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
